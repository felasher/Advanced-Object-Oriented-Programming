// Define the Student class
class Student {
  String name;
  int age;
  int gradeLevel;

  // Constructor
  Student(this.name, this.age, this.gradeLevel);

  // Method to print student information
  void printInfo() {
    print('Student Name: $name');
    print('Student Age: $age');
    print('Grade Level: $gradeLevel');
  }
}

// Define the Teacher class
class Teacher {
  String name;
  int age;
  String subject;

  // Constructor
  Teacher(this.name, this.age, this.subject);

  // Method to print teacher information
  void printInfo() {
    print('Teacher Name: $name');
    print('Teacher Age: $age');
    print('Subject: $subject');
  }
}

// Third class to create student and teacher objects
class School {
  void createObjectsAndPrintInfo() {
    // Create student object
    var student = Student('Omond', 15, 10);
    // Create teacher object
    var teacher = Teacher('Ms. Kamau', 35, 'Math');

    // Print student information
    print('Student Information:');
    student.printInfo();
    print('');

    // Print teacher information
    print('Teacher Information:');
    teacher.printInfo();
  }
}

void main() {
  // Create School object
  var school = School();
  // Call method to create objects and print information
  school.createObjectsAndPrintInfo();
}
